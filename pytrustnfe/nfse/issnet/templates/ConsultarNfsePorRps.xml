<ConsultarNfseRpsEnvio xmlns="http://www.abrasf.org.br/nfse.xsd">
	<Pedido Id="rps:{{nfse.rps.numero}}{{nfse.rps.serie}}">
		<IdentificacaoRps>
            <Numero>{{nfse.rps.numero}}</Numero>
            <Serie>{{nfse.rps.serie}}</Serie>
            <Tipo>{{nfse.rps.tipo}}</Tipo>
        </IdentificacaoRps>
		<Prestador>
            <Cnpj>{{ nfse.cnpj_prestador }}</Cnpj>
            <InscricaoMunicipal>{{ nfse.inscricao_municipal }}</InscricaoMunicipal>
        </Prestador>
	</Pedido>
</ConsultarNfseRpsEnvio>
