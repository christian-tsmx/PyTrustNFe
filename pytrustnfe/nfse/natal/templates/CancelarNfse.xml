<CancelarNfseEnvio xmlns="http://nfse.abrasf.org.br">
    <Pedido>
        <InfPedidoCancelamento>
            <IdentificacaoNfse>
                <Numero>{{nfse.rps.numero}}</Numero>
                <CpfCnpj>
                    <Cnpj>{{ nfse.cnpj_prestador }}</Cnpj>
                </CpfCnpj>
                <InscricaoMunicipal>{{ nfse.inscricao_municipal }}</InscricaoMunicipal>
                <CodigoMunicipio>{{ rps.codigo_municipio }}</CodigoMunicipio>
            </IdentificacaoNfse>
            <CodigoCancelamento>{{ nfse.codigo_cancelamento }}</CodigoCancelamento>
            <MotivoCancelamento>{{ nfse.motivo_cancelamento }}</MotivoCancelamento>
        </InfPedidoCancelamento>
    </Pedido>
</CancelarNfseEnvio>