<es:consultarNfsePorRps xmlns:es="http://www.equiplano.com.br/esnfs"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.equiplano.com.br/enfs esConsultarNfsePorRps_v01.xsd">
	<IdentificacaoRps>
        <Numero>{{nfse.rps.numero}}</Numero>
        <Serie>{{nfse.rps.serie}}</Serie>
        <Tipo>{{nfse.rps.tipo}}</Tipo>
    </IdentificacaoRps>
    <Prestador>
        <Cnpj>{{ nfse.cnpj_prestador }}</Cnpj>
        <InscricaoMunicipal>{{ nfse.inscricao_municipal }}</InscricaoMunicipal>
    </Prestador>
</es:consultarNfsePorRps>